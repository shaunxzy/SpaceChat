{
  "rules": {
    "users": {
      "$uid": {
        ".read": "auth.uid === $uid",
        ".write": "auth.uid === $uid",
        "channel": {
          ".read": "auth.uid !== null",
          ".write": "auth.uid !== null"
        }
      }
    },
    "links": {
      ".read": "auth.uid !== null",
      ".write": "auth.uid !== null",
      "$linkId": {
        ".read": "auth.uid !== null",
        ".write": "auth.uid !== null"
      }
    },
    "channel": {
      ".read": "auth.uid !== null",
      ".write": "auth.uid !== null"
    }
  }
}